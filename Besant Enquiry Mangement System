    var i=0
    var images=["https://images.pexels.com/photos/1236421/pexels-photo-1236421.jpeg?auto=compress&cs=tinysrgb&w=600","https://images.pexels.com/photos/1617610/pexels-photo-1617610.jpeg?auto=compress&cs=tinysrgb&w=600&lazy=load","https://images.pexels.com/photos/1595392/pexels-photo-1595392.jpeg?auto=compress&cs=tinysrgb&w=600&lazy=load","https://images.pexels.com/photos/1595388/pexels-photo-1595388.jpeg?auto=compress&cs=tinysrgb&w=600&lazy=load"]
    function changeImage(){
        document.getElementById("ravindra").src=images[i]
        if(i<images.length-1){
            i++
        }
       else{
        i=1
       }

       setTimeout("changeImage()",2000)

    }
    window.onload=changeImage
    const close=document.getElementById("sp")
    close.onclick=function(){
        ft.style.display="none"
    }
